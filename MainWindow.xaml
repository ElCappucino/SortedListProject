<Window x:Class="SortedListProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="196" Width="438">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="341*"/>
            <ColumnDefinition Width="97*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="KeyTxt" HorizontalAlignment="Left" Margin="55,57,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="149" Height="30"/>
        <TextBox x:Name="ValueTxt" HorizontalAlignment="Left" Margin="55,94,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="149" Height="30"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="235,0,0,0" VerticalAlignment="Center" Height="67" Width="162" Grid.ColumnSpan="2" Click="Button_Click"/>

    </Grid>
</Window>
